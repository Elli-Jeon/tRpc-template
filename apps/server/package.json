{
  "name": "server",
  "version": "1.0.0",
  "description": "trpc server",
  "main": "dist/index.js",
  "source": "src/index.ts",
  "types": "src/index.ts",
  "files": [
    "dist/*"
  ],
  "license": "MIT",
  "type": "module",
  "dependencies": {
    "typescript": "^4.5.3"
  },
  "scripts": {
    "dev": "nodemon --watch \"src/**/*.ts\" --exec ts-node --esm src/index.ts",
    "build": "tsc --project tsconfig.json",
    "start": "node dist/src/index.js"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1",
    "eslint-config-custom": "*",
    "tsconfig": "*"
  }
}
